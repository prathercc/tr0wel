<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="284.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.prath.accord.controller.ManageConversationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="listView" layoutX="14.0" layoutY="34.0" prefHeight="186.0" prefWidth="612.0" />
            <Button fx:id="selectAllButton" layoutX="14.0" layoutY="230.0" mnemonicParsing="false" onAction="#selectAll" text="Select All" />
            <Button fx:id="deleteButton" layoutX="513.0" layoutY="230.0" mnemonicParsing="false" onAction="#deleteSelections" text="Delete Selections" />
            <Text fx:id="numOfMsgText" layoutX="21.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" />
            <Text fx:id="progressText" layoutX="14.0" layoutY="273.0" strokeType="OUTSIDE" strokeWidth="0.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
