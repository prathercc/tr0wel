<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="110.0"
	prefWidth="237.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="cc.prather.tr0wel.controller.management.DeleteTabController">
	<children>
		<AnchorPane prefHeight="120.0" prefWidth="237.0">
			<children>
				<Button fx:id="deleteSelectionsButton" layoutX="65.0"
					layoutY="38.0" mnemonicParsing="false" onAction="#deleteSelections"
					text="Delete Selections" />
				<Text fx:id="progressText" layoutX="14.0" layoutY="90.0"
					strokeType="OUTSIDE" strokeWidth="0.0" />
				<Text fill="#8d8888" layoutX="7.0" layoutY="18.0"
					strokeType="OUTSIDE" strokeWidth="0.0"
					text="Note: Deleted messages are gone forever!" />
			</children>
		</AnchorPane>
	</children>
</VBox>
