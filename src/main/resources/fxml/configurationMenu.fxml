<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="202.0" prefWidth="399.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.prath.accord.controller.ConfigurationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="123.0" prefWidth="368.0" VBox.vgrow="ALWAYS">
         <children>
            <ChoiceBox fx:id="guildSelectionBox" focusTraversable="false" layoutX="43.0" layoutY="62.0" onAction="#selectGuild" prefWidth="150.0" />
            <Text layoutX="79.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Guild" />
            <Text layoutX="79.0" layoutY="109.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Channel" />
            <ChoiceBox fx:id="channelSelectionBox" disable="true" focusTraversable="false" layoutX="43.0" layoutY="113.0" onAction="#selectChannel" prefWidth="150.0" />
            <Button fx:id="manageChannelButton" disable="true" focusTraversable="false" layoutX="65.0" layoutY="147.0" mnemonicParsing="false" onAction="#manageChannel" text="Manage Channel" />
            <Text fx:id="configProgressText" layoutX="14.0" layoutY="196.0" strokeType="OUTSIDE" strokeWidth="0.0" />
            <ChoiceBox fx:id="dmUserDropDown" focusTraversable="false" layoutX="231.0" layoutY="64.0" prefHeight="25.0" prefWidth="150.0" onAction="#selectConversation"/>
            <Separator layoutX="207.0" layoutY="3.0" orientation="VERTICAL" prefHeight="182.0" prefWidth="6.0" />
            <Text layoutX="277.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select User" />
            <Button fx:id="manageDmButton" disable="true" focusTraversable="false" layoutX="240.0" layoutY="113.0" mnemonicParsing="false" text="Manage Conversation" />
            <ImageView fitHeight="46.0" fitWidth="172.0" layoutX="24.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/images/channelManage.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="46.0" fitWidth="172.0" layoutX="220.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/images/channelDirectMessages.png" />
               </image>
            </ImageView>
         </children>
    </AnchorPane>
  </children>
</VBox>
