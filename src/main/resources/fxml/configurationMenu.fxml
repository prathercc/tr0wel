<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="115.0" prefWidth="368.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.prathdev.accord.controller.ConfigurationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="123.0" prefWidth="368.0" VBox.vgrow="ALWAYS">
         <children>
            <ChoiceBox fx:id="guildSelectionBox" layoutX="14.0" layoutY="31.0" onAction="#selectGuild" prefWidth="150.0" />
            <Text layoutX="50.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Guild" />
            <Text layoutX="242.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Channel" />
            <ChoiceBox fx:id="channelSelectionBox" disable="true" layoutX="206.0" layoutY="31.0" onAction="#selectChannel" prefWidth="150.0" />
            <Button fx:id="manageChannelButton" disable="true" layoutX="131.0" layoutY="67.0" mnemonicParsing="false" onAction="#manageChannel" text="Manage Channel" />
            <Text fx:id="configProgressText" layoutX="14.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
