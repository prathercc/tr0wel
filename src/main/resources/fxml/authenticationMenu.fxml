<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="152.0" prefWidth="346.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.prathdev.accord.controller.AuthenticationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="163.0" prefWidth="206.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="emailTextField" layoutX="24.0" layoutY="50.0" promptText="Email" />
            <PasswordField fx:id="passwordTextField" layoutX="24.0" layoutY="82.0" promptText="Password" />
            <Button fx:id="authenticateButton" layoutX="223.0" layoutY="69.0" mnemonicParsing="false" onAction="#authenticate" text="Authenticate" />
            <Label layoutX="72.0" layoutY="21.0" text="Sign In" />
            <Text fx:id="progressText" layoutX="14.0" layoutY="145.0" strokeType="OUTSIDE" strokeWidth="0.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
